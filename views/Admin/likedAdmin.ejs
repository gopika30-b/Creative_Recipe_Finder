<%- include('headAdmin.ejs') %>
  <div class="g0" style="padding: 20px; border: 3px solid black;">
    <h3>Liked Recipes - Admin's</h3>
    <div class="row">
        <% Adminrecipes.forEach(recipe => { %>
            <% if (likedRecipeIds.includes(String(recipe._id))) { %>
              <div class="column">
                <div class="card">
                  <img src="<%= recipe.imageSrc %>" alt="<%= recipe.pageTitle %>" width="200px" height="200px">
                  <div class="container">
                    <h4><%= recipe.pageTitle %></h4>
                    <p class="title">
                      <% recipe.hashtags.forEach(hash => { %>
                        <%= hash %>
                      <% }); %>
                    </p>
                    <p><button class="button1" onclick="redirectTo('<%= recipe.rootURL %>', '<%= user.userID %>')">View</button></p>
                  </div>
                </div>
              </div>
            <% } %>
          <% }); %>
    </div>
  </div>
  <div class="g0" style="padding: 20px; border: 3px solid black;">
